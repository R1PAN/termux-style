    file_mtime=\$(stat -c %Y ~/.bash_history)
    current_time=\$(date +%s)
    time_threshold=230
    time_diff=\$((current_time - file_mtime))

    if [ \$time_diff -ge \$time_threshold ]; then
        rm ~/.bash_history &> /dev/null
        touch ~/.bash_history
    fi
fi

